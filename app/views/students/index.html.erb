<div class="row">
  <div class="col s12">
    <h3 class="center-align">Course Listing</h3>
  </div>
  <% @students.each do |student| %>
      <div class="col s6 m6">
          <div class="card blue-grey darken-1">
            <div class="card-content white-text">
              <span class="card-title">Name: <%=student.name%></span>
              <p>Email: <%=student.email%><p>
              <p>Course Enrollment: none</p>
            </div>
            <div class="card-action">
              <%= link_to "Profile",student_path(student) %>
              <a href="#">Courses</a>
              <a href="#">C.V</a>
            </div>
          </div>
        </div>
    <% end %>

</div>
